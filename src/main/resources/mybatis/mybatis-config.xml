<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<settings> 
        <setting name="cacheEnabled" value="true" /><!-- 全局映射器启用缓存 -->   
        <setting name="useGeneratedKeys" value="true" /> 
        <setting name="defaultExecutorType" value="REUSE" /> 
         <setting name="logImpl" value="LOG4J"/>
    </settings>
	
	<typeAliases>
		<typeAlias type="com.diverhome.entity.TbSystemUser" alias="TbSystemUser"/>
		<typeAlias type="com.diverhome.entity.TbEmployee" alias="TbEmployee"/>
		<typeAlias type="com.diverhome.entity.TbMemberShip" alias="TbMemberShip"/>
		<typeAlias type="com.diverhome.entity.TbProduct" alias="TbProduct"/>
		<typeAlias type="com.diverhome.entity.TbOrder" alias="TbOrder"/>
		<typeAlias type="com.diverhome.entity.TbNotice" alias="TbNotice"/>
		<typeAlias type="com.diverhome.entity.PageData" alias="pd"/>
	</typeAliases>
</configuration>